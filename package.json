{
  "name": "k8soketi",
  "version": "0.0.0-dev",
  "description": "Inter-planetary Soketi. Peer-to-peer WebSocket connections for Pusher and Ably.",
  "repository": {
    "type": "git",
    "url": "https://github.com/soketi/k8soketi.git"
  },
  "main": "dist/cli.js",
  "type": "module",
  "keywords": [
    "laravel",
    "socket.io",
    "broadcasting",
    "events",
    "redis",
    "socket",
    "pusher"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "npm run build -- -W",
    "cli": "NODE_OPTIONS=--es-module-specifier-resolution=node ./bin/server.js",
    "lint": "eslint --ext .js,.ts ./src",
    "prepublish": "npm run build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --runInBand --forceExit",
    "test:ci": "npm run test -- --ci",
    "test:local": "npm run test -- --verbose"
  },
  "engines": {
    "node": ">=18.0"
  },
  "author": "Alex Renoki",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@types/pusher-js": "^5.1.0",
    "@types/request": "^2.48.8",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "aws-sdk": "^2.1233.0",
    "body-parser": "^1.20.1",
    "dns2": "^2.0.5",
    "eslint": "^8.25.0",
    "express": "^4.18.2",
    "jest": "^29.2.0",
    "jest-circus": "^29.2.0",
    "pusher-js": "^7.4.0",
    "tcp-port-used": "^1.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.188.0",
    "@aws-sdk/client-lambda": "^3.188.0",
    "@aws-sdk/client-sqs": "^3.188.0",
    "@aws-sdk/util-dynamodb": "^3.188.0",
    "@chainsafe/libp2p-gossipsub": "^5.0.0",
    "@chainsafe/libp2p-noise": "^10.0.0",
    "@kubernetes/client-node": "^0.17.1",
    "@libp2p/mplex": "^7.0.0",
    "@libp2p/webrtc-direct": "^4.0.1",
    "@libp2p/websockets": "^5.0.0",
    "@rxfork/sqs-consumer": "^6.0.0",
    "arraybuffer-to-string": "^1.0.2",
    "axios": "^1.2.6",
    "colors": "^1.4.0",
    "commander": "^9.4.1",
    "dot-wild": "^3.0.1",
    "it-pipe": "^2.0.4",
    "libp2p": "^0.40.0",
    "prom-client": "^14.1.0",
    "pusher": "^5.1.1-beta",
    "query-string": "^7.1.1",
    "rate-limiter-flexible": "^2.3.12",
    "uWebSockets.js": "https://github.com/uNetworking/uWebSockets.js.git#v20.10.0"
  },
  "bin": {
    "k8soketi": "bin/server.js"
  }
}
